{
  "name": "o3-shop/shop-doctrine-migration-wrapper",
  "description": "O3-Shop database migrations using doctrine",
  "license": "GPL-3.0-only",
  "author": [
    {
      "name": "OXID eSales AG",
      "email": "info@oxid-esales.com",
      "homepage": "https://oxid-esales.com",
      "role": "Developer"
    },
    {
      "name": "O3-Shop",
      "email": "info@o3-shop.com",
      "homepage": "https://www.o3-shop.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "doctrine/migrations": "^2.3.2",
    "composer/package-versions-deprecated": "^1",
    "o3-shop/shop-facts": "^1.0.0",
    "symfony/console": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.14",
    "mikey179/vfsstream": "~1.6.8",
    "symfony/filesystem": "^4.4.17",
    "symfony/yaml": "~3.4 || ~4.0",
    "o3-shop/shop-ce": "^1.0.0",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "OxidEsales\\DoctrineMigrationWrapper\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OxidEsales\\DoctrineMigrationWrapper\\Tests\\": "./tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "./bin/oe-eshop-db_migrate",
    "./bin/oe-eshop-doctrine_migration"
  ],
  "replace": {
    "oxid-esales/oxideshop-doctrine-migration-wrapper": "3.3.0"
  }
}
